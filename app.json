{
  "expo": {
    "name": "Txtend",
    "slug": "txtend",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/logo1.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.txtend.myapp"
    },
    "android": {
      "permissions": [],
      "package": "com.txtend.myapp",
      "versionCode": 5
    }
  },
  "web": {
    "favicon": "./assets/images/logo1.png"
  }
}
