{
  "expo": {
    "name": "Txtend",
    "slug": "txtend",
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/images/android_logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splas_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.txtend.myapp"
    },
    "android": {
      "permissions": ["ACCESS_MEDIA_LOCATION","CAMERA","WRITE_EXTERNAL_STORAGE","READ_EXTERNAL_STORAGE","RECORD_AUDIO","SYSTEM_ALERT_WINDOW"],
      "package": "com.txtend.myapp",
      "versionCode": 11
      // "googleServicesFile": "./google-services.json"
  },
  "web": {
    "favicon": "./assets/images/logo1.png"
  },
  "plugins": 
    ["@react-native-firebase/app"]
  }
}
