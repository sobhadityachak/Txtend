{
  "expo": {
    "name": "Txtend",
    "slug": "txtend",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/android_logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splas_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.interesingTechnologies.txtend"
    },
    "android": {
      "permissions": [],
      "package": "com.interestingTechnologies.txtend",
      "versionCode": 5,
      "googleServiceFiles": "./google-services.json"
  },
  "web": {
    "favicon": "./assets/images/logo1.png"
  },
  "plugins": 
    ["@react-native-firebase/app"]
  }
}
